{
  "name": "seed-toolkit",
  "version": "1.0.15",
  "description": "One step generating pages from data",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --dest dist --name seed-toolkit packages/index.js",
    "docs:dev": "npx vuepress dev docs",
    "docs:build": "npx vuepress build docs",
    "test:unit": "vue-cli-service test:unit",
    "postversion": "git push --follow-tags",
    "lint": "vue-cli-service lint",
    "changelog": "npx conventional-changelog -p angular -i ./dist/CHANGELOG.md -s -r 0"
  },
  "license": "MIT",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "author": {
    "name": "Edward Wu",
    "email": "mizuka.wu@outlook.com",
    "url": "https://www.mizuka.top"
  },
  "files": [
    "packages",
    "dist",
    "lib",
    "README.md",
    "types",
    "LICENSE"
  ],
  "main": "dist/seed-toolkit.umd.min.js",
  "keywords": [
    "vue",
    "seed",
    "vue2",
    "form",
    "element-ui"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mizuka-wu/seed"
  },
  "typings": "typings/index.d.ts",
  "dependencies": {
    "ant-design-vue": "^1.2.4",
    "dayjs": "^1.8.31",
    "element-ui": "^2.4.5",
    "exceljs": "3.3.1",
    "file-saver": "^2.0.2",
    "immutable": "^4.0.0-rc.12",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "lodash-es": "^4.17.15",
    "pretty-bytes": "^5.3.0",
    "sortablejs": "^1.10.2",
    "uuid": "^7.0.2",
    "vue": "^2.6.12",
    "vue-route": "^1.5.1"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.10.4",
    "@vue/cli-plugin-babel": "^4.2.0",
    "@vue/cli-plugin-eslint": "^4.2.0",
    "@vue/cli-plugin-unit-mocha": "^5.0.8",
    "@vue/cli-service": "^4.2.0",
    "@vue/composition-api": "^0.3.4",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "1.0.0-beta.31",
    "babel-eslint": "^10.0.3",
    "chai": "^4.1.2",
    "conventional-changelog-cli": "^2.0.31",
    "core-js": "^3.6.5",
    "defer-to-connect": "^2.0.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^6.1.2",
    "lint-staged": "^10.2.11",
    "mockjs": "^1.1.0",
    "prettier": "^1.19.1",
    "prismjs": "^1.20.0",
    "qs": "^6.9.4",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "vue-cli-plugin-ant-design": "^1.0.0",
    "vue-cli-plugin-element": "^1.0.1",
    "vue-clipboard2": "^0.3.1",
    "vue-template-compiler": "^2.6.12",
